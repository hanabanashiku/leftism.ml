---
layout: none
---
[
{% for page in site.pages %}
{
{% if page.includeInSearch == true %}
"title"    : "{{ page.title | escape }}",
"category" : "{{ page.category }}",
"tags"     : "{{ page.tags | join: ', ' }}",
"url"      : "{{ site.baseurl }}{{ page.url }}",
"date"     : "{{ page.date }}",
"content"  : "{{ page.content | strip_html | strip_newlines }}"
{% endif %}
} {% unless forloop.last %},{% endunless %}
{% endfor %}
]